    public void createSummaryEntry() {
        int g = group.size();
        int ap;
        for (int i = 0; i < g; i++) {
            ap = Group.countActiveProjects();
        }
    }

    public void countOpenActivities() {
        int a = openActivities.size();
        for(int i = 0; i<a; i++){
            openActivities.get(i).isActive()
        }
    }
